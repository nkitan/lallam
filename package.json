{
  "name": "llamachat",
  "version": "1.0.0",
  "description": "A High-Performance, On-Device LLM Chat Application",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace LlamaChat.xcworkspace -scheme LlamaChat -configuration Release -sdk iphoneos build"
  },
  "dependencies": {
    "@nozbe/watermelondb": "^0.27.1",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "react": "18.2.0",
    "react-native": "0.75.0",
    "react-native-gesture-handler": "^2.18.1",
    "react-native-get-random-values": "^1.11.0",
    "react-native-mmkv": "^3.0.2",
    "react-native-reanimated": "^3.15.0",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^3.34.0",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-vector-icons": "^10.1.0",
    "uuid": "^9.0.1",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@babel/preset-env": "^7.25.4",
    "@babel/runtime": "^7.25.4",
    "@react-native-community/cli": "^15.0.0",
    "@react-native-community/cli-platform-android": "^15.0.0",
    "@react-native-community/cli-platform-ios": "^15.0.0",
    "@react-native/eslint-config": "^0.75.0",
    "@react-native/metro-config": "^0.75.0",
    "@tsconfig/react-native": "^3.0.5",
    "@types/react": "^18.3.4",
    "@types/react-test-renderer": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.2.0",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "react-native",
    "llm",
    "ai",
    "chat",
    "on-device",
    "privacy"
